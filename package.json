{
  "name": "@graffiti-garden/wrapper-vue",
  "version": "0.0.5",
  "description": "Vue.js wrapper for the Graffiti API",
  "type": "module",
  "main": "dist/plugin.js",
  "browser": "dist/plugin.js",
  "exports": {
    ".": {
      "types": "./src/plugin.ts",
      "default": "./dist/plugin.js"
    }
  },
  "files": [
    "src",
    "dist",
    "package.json",
