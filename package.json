  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graffiti-garden/wrapper-vue.git"
  },
  "bugs": {
    "url": "https://github.com/graffiti-garden/wrapper-vue/issues"
  },
  "dependencies": {
    "@graffiti-garden/api": "^0.1.11"
  },
  "devDependencies": {
    "@graffiti-garden/implementation-local": "^0.2.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/language-server": "^2.2.0",
    "@vue/typescript-plugin": "^2.2.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
